<div class="col-6">
   <h1>Productos en el carrito</h1>
   <table class="table">
      <thead>
         <tr id="primera">
            <th id="encabezado" scope="col"> Nombre</th>
            <th scope="col"> Descripcion</th>
            <th scope="col">Precio</th>
            <th scope="col">Codigo</th>
            <th scope="col">Stock</th>
            <th scope="col">Categoria</th>
            <th scope="col">Cantidad</th>
        
            </tr>
      </thead>
        <tbody>
    
      {{#each productos}}
            <td>{{id_prod.title}}</td>
            <td>{{id_prod.description}}</td>
            <td>{{id_prod.price}}</td>
            <td>{{id_prod.code}} </td>
            <td>{{id_prod.stock}} </td>  
            <td>{{id_prod.category}} </td>
            <td>{{quantity}} </td>
         
         </tr>
         {{/each}}
      </tbody>
   </table>
</div>